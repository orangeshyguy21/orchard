{
  "name": "orchard",
  "version": "1.6.2",
  "description": "Web application for Cashu mint management",
  "author": "shyguy",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "build": "npx ts-node -r tsconfig-paths/register --project tsconfig.server.json scripts/generate-schema.ts && NODE_ENV=production npx graphql-codegen && NODE_ENV=production nest build && NODE_ENV=production ng build",
    "format": "prettier --write \"src/client/**/*.{ts,html}\" \"src/client/modules/**/*.scss\" \"src/server/**/*.ts\"",
    "format:watch": "nodemon --watch src/client --watch src/server --ext ts,html,scss,json --exec \"npm run format\"",
    "format:check": "prettier --check \"src/client/**/*.{ts,html}\" \"src/client/modules/**/*.scss\" \"src/server/**/*.ts\"",
    "ng": "ng",
    "start": "NODE_ENV=production node ./scripts/generate-runtime-config.mjs && NODE_ENV=production node dist/main",
    "start:client": "NODE_ENV=development node ./scripts/generate-runtime-config.mjs && NODE_ENV=development npx graphql-codegen && NODE_ENV=development ng serve --proxy-config proxy.conf.js",
    "start:server": "NODE_ENV=development nest start --watch",
    "start:server:nowatch": "NODE_ENV=development nest start",
    "start:server:debug": "NODE_ENV=development nest start --debug --watch",
    "generate:types": "npx ts-node -r tsconfig-paths/register --project tsconfig.server.json scripts/generate-schema.ts && npx graphql-codegen",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest && ng test --watch=false --browsers=ChromeHeadless --progress=false",
    "test:server": "jest",
    "test:server:watch": "jest --watch",
    "test:server:cov": "jest --coverage",
    "test:server:e2e": "jest --config ./test/jest-e2e.json",
    "test:client": "ng test --watch=false --browsers=ChromeHeadless --progress=false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "watch:client": "NODE_ENV=development ng build --watch --configuration development",
    "migration:generate": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d src/server/database/data-source.ts",
    "migration:run": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/server/database/data-source.ts",
    "migration:revert": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/server/database/data-source.ts",
    "migration:show": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:show -d src/server/database/data-source.ts",
    "debug:ln-analytics": "npx ts-node -r tsconfig-paths/register --project tsconfig.server.json scripts/debug-ln-analytics.ts"
  },
  "dependencies": {
    "@angular/animations": "^21.1.4",
    "@angular/cdk": "^21.1.4",
    "@angular/common": "^21.1.4",
    "@angular/compiler": "^21.1.4",
    "@angular/core": "^21.1.4",
    "@angular/forms": "^21.1.4",
    "@angular/material": "^21.1.4",
    "@angular/material-luxon-adapter": "^21.1.4",
    "@angular/platform-browser": "^21.1.4",
    "@angular/platform-browser-dynamic": "^21.1.4",
    "@angular/router": "^21.1.4",
    "@apollo/server": "^5.4.0",
    "@as-integrations/express5": "^1.1.2",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/apollo": "13.2.4",
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.1.13",
    "@nestjs/graphql": "13.2.4",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "chart.js": "^4.5.1",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "compression": "^1.8.1",
    "graphql": "^16.12.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tools": "^9.0.26",
    "luxon": "^3.7.2",
    "marked": "^17.0.2",
    "ng2-charts": "^8.0.0",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "qr-code-styling": "^1.9.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socks-proxy-agent": "^8.0.5",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.28",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.4",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/jasmine": "~6.0.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.13.10",
    "@types/node-fetch": "^2.6.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jasmine-core": "~6.0.1",
    "jest": "^30.2.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/server",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.server.spec.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^@server/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "@apollo/server-plugin-landing-page-graphql-playground": {
      "@apollo/server": "^5.0.0"
    }
  }
}
