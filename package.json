{
  "name": "orchard",
  "version": "1.5.0",
  "description": "Web application for Cashu mint management",
  "author": "shyguy",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "build": "npx ts-node -r tsconfig-paths/register --project tsconfig.server.json scripts/generate-schema.ts && NODE_ENV=production npx graphql-codegen && NODE_ENV=production nest build && NODE_ENV=production ng build",
    "format": "prettier --write \"src/client/**/*.{ts,html}\" \"src/client/modules/**/*.scss\" \"src/server/**/*.ts\"",
    "format:watch": "nodemon --watch src/client --watch src/server --ext ts,html,scss,json --exec \"npm run format\"",
    "format:check": "prettier --check \"src/client/**/*.{ts,html}\" \"src/client/modules/**/*.scss\" \"src/server/**/*.ts\"",
    "ng": "ng",
    "start": "NODE_ENV=production node ./scripts/generate-runtime-config.mjs && NODE_ENV=production node dist/main",
    "start:client": "NODE_ENV=development node ./scripts/generate-runtime-config.mjs && NODE_ENV=development npx graphql-codegen && NODE_ENV=development ng serve --proxy-config proxy.conf.js",
    "start:server": "NODE_ENV=development nest start --watch",
    "start:server:nowatch": "NODE_ENV=development nest start",
    "start:server:debug": "NODE_ENV=development nest start --debug --watch",
    "generate:types": "npx ts-node -r tsconfig-paths/register --project tsconfig.server.json scripts/generate-schema.ts && npx graphql-codegen",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest && ng test --watch=false --browsers=ChromeHeadless --progress=false",
    "test:server": "jest",
    "test:server:watch": "jest --watch",
    "test:server:cov": "jest --coverage",
    "test:server:e2e": "jest --config ./test/jest-e2e.json",
    "test:client": "ng test --watch=false --browsers=ChromeHeadless --progress=false",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "watch:client": "NODE_ENV=development ng build --watch --configuration development",
    "migration:generate": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -d src/server/database/data-source.ts",
    "migration:run": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/server/database/data-source.ts",
    "migration:revert": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert -d src/server/database/data-source.ts",
    "migration:show": "NODE_ENV=development ts-node --project tsconfig.server.json -r tsconfig-paths/register node_modules/typeorm/cli.js migration:show -d src/server/database/data-source.ts"
  },
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/material": "^20.2.10",
    "@angular/material-luxon-adapter": "^20.2.10",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@apollo/server": "^4.12.2",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/apollo": "13.1.0",
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/graphql": "13.1.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.1",
    "chartjs-adapter-luxon": "^1.3.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tools": "^9.0.20",
    "luxon": "^3.7.2",
    "marked": "^16.4.1",
    "ng2-charts": "^8.0.0",
    "node-fetch": "^2.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "qr-code-styling": "^1.9.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socks-proxy-agent": "^8.0.5",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.27",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.12",
    "@angular/build": "^20.3.12",
    "@angular/cli": "^20.3.12",
    "@angular/compiler-cli": "^20.3.15",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typescript": "^5.0.2",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.7",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.12",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.13.10",
    "@types/node-fetch": "^2.6.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.12.0",
    "jest": "^30.2.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src/server",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.server.spec.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^@server/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}